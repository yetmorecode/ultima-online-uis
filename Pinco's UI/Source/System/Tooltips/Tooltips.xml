	<Windows>
		
		<Window name="TooltipRow" layer="overlay">
			<Windows>
				<Label name="$parentCol1Text" inherits="UO_Default_Label_Color" font="MyriadPro_16" maxchars="2048" textalign="topleft" autoresize="true" wordwrap="true">
					<Size>
						<AbsPoint x="0" y="0" />
					</Size>
					<Anchors>
						<Anchor point="topleft" relativePoint="topleft">
							<AbsPoint x="0" y="0" />
						</Anchor>
					</Anchors>	
					<Color r="255" g="255" b="255" a="255" />
				</Label>
				                              
