function CreaturesDB.Initialize()
	local q = 0
	for key, val in pairsByKeys (CreaturesDB) do
		if (type(val) ~= "function") then
			if (val.tamable) then
				q = q + 1
				CreaturesDB.AnimalLoreTamables[q] = StringToWString(key)
			end
		end
	end
	SortFunc(CreaturesDB.AnimalLoreTamables)
	CreaturesDB.updateSummonTimes()
end

function CreaturesDB.updateSummonTimes()
	serverId = WindowData.SkillsCSV[32].ServerId
	local magery = WindowData.SkillDynamicData[serverId].TempSkillValue / 10
	
	serverId = WindowData.SkillsCSV[46].ServerId
	local sw = WindowData.SkillDynamicData[serverId].TempSkillValue
	
	serverId = WindowData.SkillsCSV[37].ServerId
	local mystic = WindowData.SkillDynamicData[serverId].RealSkillValue
	
	serverId = WindowData.SkillsCSV[26].ServerId
	local imbuing = WindowData.SkillDynamicData[serverId].RealSkillValue 
	
	serverId = WindowData.SkillsCSV[21].ServerId
	local focus = WindowData.SkillDynamicData[serverId].RealSkillValue 
	
	serverId = WindowData.SkillsCSV[47].ServerId
	local sspeak = WindowData.SkillDynamicData[serverId].RealSkillValue
	
	serverId = WindowData.SkillsCSV[39].ServerId
	local ninj = WindowData.SkillDynamicData[serverId].RealSkillValue
	
	local focusImb = math.max(focus, imbuing)
	
	CreaturesDB.SummonTimes["a revenant"] = ((sspeak * 80) / 1200) + 10
	
	CreaturesDB.SummonTimes["an energy vortex"] = 90 
	CreaturesDB.SummonTimes["a blade spirit"] = 120
	
	CreaturesDB.SummonTimes["summon creature"] =  ((20 * magery) / 5)
	
	CreaturesDB.SummonTimes["an earth elemental"] = ((20 * magery) / 5) 
	CreaturesDB.SummonTimes["an air elemental"] = ((20 * magery) / 5) 
	CreaturesDB.SummonTimes["a water elemental"] = ((20 * magery) / 5) 
	CreaturesDB.SummonTimes["a fire elemental"] = ((20 * magery) / 5) 

	CreaturesDB.SummonTimes["daemon"] = ((20 * magery) / 5) - 2

	CreaturesDB.SummonTimes["a rising colossus"] = ((mystic + focusImb) / 40)
	CreaturesDB.SummonTimes["an animated weapon"] = ((mystic + focusImb) / 20) + 10
	
	CreaturesDB.SummonTimes["a nature's fury"] =  (sw / 240) + (2 * Interface.ArcaneFocusLevel) + 25
	
	CreaturesDB.SummonTimes["an imp"] = (((sw / 240) +  Interface.ArcaneFocusLevel) * 60) 
	CreaturesDB.SummonTimes["fey"] =  (((sw / 240) +  Interface.ArcaneFocusLevel) * 60) 
	
	CreaturesDB.SummonTimes["mirror image"] =  30 + (ninj / 40)
	
	CreaturesDB.SummonTimes["talisman summon"] =  600
end



function SortFunc (myTable)
    local t = {}
    for title,value in pairsByKeys(myTable) do
        table.insert(t, { title = title, value = value })
    end
myTable = t
    return myTable
end

function CreaturesDB.GetName(id)
	if not id or not IsMobile(id) then
		return
	end
	if not WindowData.MobileName[id] then
		RegisterWindowData(WindowData.MobileName.Type, id)
	end
	if not WindowData.MobileName[id] then
		return
	end
	
	local name = WindowData.MobileName[id].MobName
	
	if (type(name) == "wstring") then
		name = WStringToString(name)
	end
	
	name = string.gsub(name, " An ", "")
	name = string.gsub(name, " A ", "")
	
	name = string.gsub(name, "\n", " ")
	
	text = string.find(name , "Paragon")
	if ( text ~= nil ) then
		name = string.gsub(name, "Paragon", "")
		name = string.gsub(name, "[(]", "")
		name = string.gsub(name, "[)]", "")
	end
	
	text = string.find(name , "[(]Changeling[)]")
	if ( text ~= nil ) then
		name = "Changeling"
	end
	
	text = string.find(name , "[(]Irk[)]")
	if ( text ~= nil ) then
		name = "Irk"
	end
	
	text = string.find(name , "[(]Guile[)]")
	if ( text ~= nil ) then
		name = "Guile"
	end
	
	text = string.find(name , "[(]Spite[)]")
	if ( text ~= nil ) then
		name = "Spite"
	end
	
	text = string.find(name , "[(]Travesty[)]")
	if ( text ~= nil ) then
		name = "Travesty"
	end
	
	text = string.find(name , "Guardian")
	if ( text ~= nil and string.find(name , "[(]")) then
		name = string.gsub(name, "Guardian", "")
		name = string.gsub(name, "[(]", "")
		name = string.gsub(name, "[)]", "")
	end
	--Debug.Print("!" .. name .. "!")
	text = string.find(name , "Renowned")
	if ( text ~= nil ) then
		name = string.sub(name,1, text - 2)
		name = name .. "renowned"
	end
	
	text = string.find(name , "[[]CoM[]]")
	if ( text ~= nil ) then
		name = string.sub(name,1, text - 2)
	end
	
	text = string.find(name , "[[]TB[]]")
	if ( text ~= nil ) then
		name = string.sub(name,1, text - 2)
	end
	
	text = string.find(name , "[[]SL[]]")
	if ( text ~= nil ) then
		name = string.sub(name,1, text - 2)
	end
	
	text = string.find(name , "[[]Min[]]")
	if ( text ~= nil and not string.find(name , "Minotaur") and string.len(string.sub(name,1, text - 2)) > 0 ) then
		name = string.sub(name,1, text - 2)
	end
	
	local noto = 0
	if (WindowData.MobileName[id]) then
		noto = WindowData.MobileName[id].Notoriety+1
	end
	
	name = string.gsub(name, "^%s*(.-)%s*$", "%1")
	name = string.lower(name)
	
	name = CreaturesDB.NameToType(name)
	if ((CreaturesDB[name] == nil or name == "hell hound") and noto == NameColor.Notoriety.MURDERER) then
		name = name .. " - RED"
	end
	
	if (CreaturesDB[name] ~= nil and IsMobile(id)) then
		return name
	end
end

