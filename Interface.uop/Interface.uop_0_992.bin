function CreaturesDB.GetBardDiff(name)
	if not name then
		return L""
	end
	if (CreaturesDB[name] ~= nil and CreaturesDB[name].barddiff) then
		local bd = WStringToString(CreaturesDB[name].barddiff)
		if bd then
			bd = string.gsub(bd, ",", ".")
			return StringToWString(bd)
		end
	end
	return L""

end

