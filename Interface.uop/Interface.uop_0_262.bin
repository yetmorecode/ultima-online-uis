		<!-- STANDARD SCROLL BAR !!! -->
		<VerticalScrollbar name="LoginVertScroll" up="LoginVertScrollUpBtn" down="LoginVertScrollDownBtn" thumb="LoginVertScrollThumb" gutter="LoginVertScrollGutter" >
