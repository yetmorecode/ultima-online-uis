					<Size>
						<AbsPoint x="0" y="0" />
					</Size>
					<Anchors>
						<Anchor point="right" relativePoint="left" relativeTo="$parentCol1Text">
							<AbsPoint x="0" y="0" />
						</Anchor>
					</Anchors>
					<Color r="255" g="255" b="255" a="255" />
				</Label>
			</Windows>
      
		</Window>

